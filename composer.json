{
    "name": "katya/test9.mvc_framework",
    "description": "Simple MVC_framework",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/Katrin9876/PHP.MVC-framework/issues",
        "source": "https://github.com/Katrin9876/PHP.MVC-framework",
        "wiki": "https://github.com/Katrin9876/PHP.MVC-framework/wiki"
    },
    "authors": [
        {
            "name": "Katerin",
            "email": "polkhovskaia.katerina@gmail.com"
        }
    ],
    "minimum-stability": "alpha",
    "require": {
        "php": "8.1.24|^8.0",
        "ext-mbstring": "*",
        "ext-iconv": "*",
        "ext-pdo": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "psr-4": {
            "Main\\": "src/Main"
        }
    },
    "autoload-dev":{
        "psr-4":{
            "Main\\Tests\\": "tests/"
        }
    }
}
